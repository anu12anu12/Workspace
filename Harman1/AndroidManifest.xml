<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.harman11"
      android:versionCode="1"
      android:versionName="1.0">
<uses-permission android:name="com.harmanhero.HarmanProvider"></uses-permission>


    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".Harman1"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".HarmanService">
            <intent-filter>
                <action android:name="android.intent.action.harmanservice"></action>
                <category android:name="android.intent.action.DEFAULT"></category>
            </intent-filter>
        </service>
        

 
     </application>
</manifest>